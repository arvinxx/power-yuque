{
  "name": "power-yuque",
  "version": "0.4.0",
  "description": "a chrome plugin to enhance yuque",
  "private": true,
  "author": "Arvin Xu(arvinx@foxmail.com)",
  "license": "MIT",
  "homepage": "https://github.com/arvinxx/power-yuque#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/arvinxx/power-yuque.git"
  },
  "bugs": {
    "url": "https://github.com/arvinxx/power-yuque/issues"
  },
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "zip": "yarn build && yarn zip:only",
    "zip:only": "ts-node ./scripts/zip",
    "---------  ": "",
    "test": "umi-test",
    "test:update": "UPDATE=1 umi-test --update-snapshot",
    "test:coverage": "umi-test --coverage",
    "---------   ": "",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s -r 0",
    "release": "semantic-release",
    "---------    ": "",
    "clean": "rm -rf es lib dist build coverage e2e/dist packages/.umi .eslintcache",
    "postinstall": "umi generate tmp",
    "lint": "npm run lint-eslint",
    "tsc": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty src",
    "lint-staged": "lint-staged",
    "lint-styles": "stylelint",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prettier": "prettier -c --write \"**/**\""
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint -c .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint   --fix"
    ],
    "*.{ts,tsx,json,js,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.0.17",
    "@ant-design/pro-layout": "^6.14.0",
    "@ant-design/pro-list": "^1.5.0",
    "@umijs/preset-react": "^1.7.13",
    "@yuque/sdk": "^1.1.1",
    "ahooks": "^2.9.6",
    "antd": "^4.12.3",
    "classnames": "^2.2.6",
    "current-device": "^0.10.2",
    "dayjs": "^1.10.4",
    "jquery": "^3.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hotkeys-hook": "^3.1.0",
    "react-intl": "^5.12.3",
    "react-spring": "^8.0.27",
    "rxjs": "^6.6.3",
    "rxjs-hooks": "^0.6.2",
    "umi": "^3.3.7",
    "umi-plugin-extensions": "^0.4.0",
    "umi-request": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/archiver": "^5.1.0",
    "@types/chrome": "^0.0.130",
    "@types/classnames": "^2.2.11",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.14",
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.28",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webpack": "^4.41.22",
    "@umijs/fabric": "^2.5.6",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.3.7",
    "archiver": "^5.2.0",
    "babel-jest": "^26.6.0",
    "commitlint-config-gitmoji": "^2.2.1",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-gitmoji-config": "^1.4.0",
    "cross-env": "^7.0.0",
    "eslint": "^7.20.0",
    "eslint-config-umi": "^1.6.0",
    "fs-extra": "^9.1.0",
    "husky": "^4.3.8",
    "jest": "26.6.3",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "semantic-release": "^17.3.9",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  }
}
